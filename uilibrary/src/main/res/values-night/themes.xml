    <style name="AppTheme" parent="AppTheme.NoStatusBar"></style>

    <style name="AppTheme.NoStatusBar" parent="AppTheme.TextAppearance">
        <!--        edge to edge    -->
        <item name="android:windowBackground">@color/md_theme_dark_background</item>
        <!-- 顶部状态栏  Status bar color. -->
        <item name="android:statusBarColor">@android:color/transparent</item>
        <!-- 底部操作栏  navigationBarColor-->
        <item name="android:navigationBarColor">@android:color/transparent</item>
        <!-- Optional: set the status bar light and content dark. 设置状态栏文字色为深色, true为浅色背景深色文字, false为深色背景浅色文字-->
        <item name="android:windowLightStatusBar">false</item>
        <!-- Optional: set the navigation bar light and content dark. 设置状态栏文字色为深色, true为浅色背景深色文字, false为深色背景浅色文字-->
        <!--        <item name="android:windowLightNavigationBar">true</item>-->

        <!--        窗口（Activity）切换动画-->
        <item name="android:windowAnimationStyle">@style/default_animation</item>

        <!-- Primary brand color. -->
        <item name="colorPrimaryVariant">@color/md_theme_dark_primary</item>
        <item name="colorAccent">@color/md_theme_dark_secondary</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondaryVariant">@color/md_theme_dark_secondary</item>

    </style>

